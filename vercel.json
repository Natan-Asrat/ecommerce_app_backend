{
    "builds": [
      {
        "src": "application2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "1024mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "deploy.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
        
      {
        "src": "/(.*)",
        "dest": "application2/wsgi.py"
      }
      
    ]
  }