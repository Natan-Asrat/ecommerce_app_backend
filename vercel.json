{
    "builds": [
      {
        "src": "application2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "1024mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
        {
            "src": "/deploy",
            "dest": "deploy.sh",
            "methods": ["GET"]
          },
      {
        "src": "/(.*)",
        "dest": "application2/wsgi.py"
      }
      
    ]
  }