{
    "builds": [
        {
            "src": "application2/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "1024mb", "runtime": "python3.9"}        
        },
        {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build/static"
        }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "application2/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          }
    ]
}