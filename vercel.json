{
    "builds": [
      {
        "src": "application2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "1024mb",
          "runtime": "python3.9"
        }
    },
    {
        "src": "deploy.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
      
      
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
      {
        "src": "/(.*)",
        "dest": "application2/wsgi.py"
      }
      
    ]
  }